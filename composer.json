{
  "name":         "evozon-php/tissue-bundle",
  "type":         "library",
  "description":  "Slight variation of the original cleentfaar/CLTissueBundle",
  "keywords":     ["anti-virus", "security", "tissue", "virus", "clamav"],
  "homepage":     "http://evozon.com",
  "license":      "proprietary",
  "authors": [
    {
      "name": "Cas Leentfaar",
      "homepage": "https://github.com/cleentfaar",
      "email": "info@casleentfaar.com"
    },
    {
      "name": "Balazs Csaba",
      "homepage": "http://evozon.com",
      "email": "csaba.balazs@evozon.com"
    }
  ],
  "require":      {
    "php": ">=7.1",
    "symfony/symfony": "^3.0 || ^4.0",
    "symfony/options-resolver": "^3.0 || ^4.0",
    "symfony/validator": "^3.0 || ^4.0",
    "evozon-php/tissue": "*",
    "evozon-php/tissue-clamav-adapter": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "matthiasnoback/symfony-config-test": "^3.1"
  },
  "suggests": {
    "cleentfaar/tissue-clamavphp-adapter": "If you want to scan your files using the PHP-extension of the ClamAV engine"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload":     {
    "psr-4": {
      "CL\\Bundle\\TissueBundle\\": ""
    }
  }
}
